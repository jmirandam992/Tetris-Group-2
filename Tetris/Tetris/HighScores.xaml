        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="250,505,0,0" VerticalAlignment="Top" Width="100" Click="BackButton_Click" Background="White" BorderBrush="#FF33F4FD" Foreground="#FF33F4FD" BorderThickness="5" FontFamily="Broadway" FontWeight="Bold" FontStyle="Italic" Height="50"/>
        <Label x:Name="lblHeader" Content="Top 10 High Scores" HorizontalAlignment="Left" Height="38" Margin="170,68,0,0" VerticalAlignment="Top" Width="284" FontFamily="Broadway" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="#FF33F4FD"/>
        <Label x:Name="lbl1st" Content="1st" HorizontalAlignment="Left" Height="38" Margin="60,111,0,0" VerticalAlignment="Top" Width="474" FontFamily="Broadway" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="#FF33F4FD"/>
        <Label x:Name="lbl2nd" Content="2nd" HorizontalAlignment="Left" Height="38" Margin="60,149,0,0" VerticalAlignment="Top" Width="474" FontFamily="Broadway" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="#FF33F4FD"/>
