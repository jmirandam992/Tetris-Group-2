        <Label x:Name="lbl4th" Content="4th" HorizontalAlignment="Left" Height="38" Margin="60,225,0,0" VerticalAlignment="Top" Width="474" FontFamily="Broadway" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="#FF33F4FD"/>
        <Label x:Name="lbl5th" Content="5th" HorizontalAlignment="Left" Height="38" Margin="60,263,0,0" VerticalAlignment="Top" Width="474" FontFamily="Broadway" FontSize="24" FontWeight="Bold" FontStyle="Italic" Foreground="#FF33F4FD"/>
